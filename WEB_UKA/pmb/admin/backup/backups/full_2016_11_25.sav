insert into infopages (id_infopage, content_infopage, title_infopage, valid_infopage, restrict_infopage) values (1, "Actualités de la BnF\r\n</h1>\r\n<ul>\r\n<!--load asset-->\r\n<!--Get referenced by list-->\r\n<li>\r\n<p class=\"echeance\">\r\n8 mars 2010\r\n</p>\r\n<p class=\"titre\">\r\n<a href=\"http://www.bnf.fr/fr/la_bnf/anx_actu_bib/a.cabinet_monnaies_100308.html\">Lavenir du « Cabinet des Médailles »</a>	\r\n</p>\r\n<p class=\"maj\">[mis en ligne le 8 mars 2010 ]</p>\r\n\r\n</li>\r\n<!--load asset-->\r\n<!--Get referenced by list-->\r\n<li>\r\n<p class=\"echeance\">\r\njusqu\'à nouvel avis\r\n</p>\r\n<p class=\"titre\">\r\n<a href=\"http://www.bnf.fr/fr/la_bnf/renovation_richelieu/a.communication_differee_collections_richelieu.html\">Communication différée de certaines collections sur le site Richelieu</a>	\r\n</p>\r\n<p class=\"lieu\">Site\r\nRichelieu\r\n</p>\r\n<p class=\"maj\">[mis en ligne le 7 janvier 2010 ]</p>\r\n</li>\r\n<!--load asset-->\r\n\r\n<li>\r\n<span class=\"cartouche_breve\">\r\nNouveau\r\n</span> \r\n<p class=\"titre\">\r\n<a href=\"http://enfants.bnf.fr/\">La Bibliothèque numérique des enfants</a>	\r\n</p>\r\nEspace entièrement dédié aux jeunes de 8 à 12 ans, la Bibliothèque numériques des enfants est une véritable introduction à l\'univers de l\'écrit et de l\'image.\r\n<p class=\"maj\">[mis en ligne le 26 juillet 2010 ]</p>\r\n</li>\r\n<!--load asset-->\r\n<!--Get referenced by list-->\r\n<li>\r\n<p class=\"echeance\">\r\njusqu\'au 5 septembre 2010\r\n\r\n</p>\r\n<p class=\"titre\">\r\n<a href=\"http://www.bnf.fr/fr/collections_et_services/actus_coll_dpts/s.actualites_sciences_techniques.html?first_Art=non\">Épidémies, pandémies  Sélection de documents et bibliographie</a>	\r\n</p>\r\n<p class=\"lieu\">Site\r\nFrançois-Mitterrand\r\n<span> -\r\nBibliothèque détude (niveau Haut-de-jardin)\r\n</span> \r\n</p>\r\n<p class=\"maj\">[mis en ligne le 16 juillet 2010 ]</p>\r\n</li>\r\n<!--load asset-->\r\n<!--Get referenced by list-->\r\n<li>\r\n<p class=\"echeance\">\r\n\r\njusqu\'au 5 septembre 2010\r\n</p>\r\n<p class=\"titre\">\r\n<a href=\"http://www.bnf.fr/fr/collections_et_services/actus_coll_dpts/s.actualites_audiovisuel.html?first_Art=non\">Image par image  Sélection de films</a>	\r\n</p>\r\n<p class=\"lieu\">Site\r\nFrançois-Mitterrand\r\n<span> -\r\nBibliothèque détude (niveau Haut-de-jardin)\r\n</span> \r\n</p>\r\n<p class=\"maj\">[mis en ligne le 12 juillet 2010 ]</p>\r\n</li>\r\n</ul>", "Actu rencontres", 1, 0);
insert into infopages (id_infopage, content_infopage, title_infopage, valid_infopage, restrict_infopage) values (2, "<img src=./styles/pmb34/images/iconepmb34.png width=100px height=100px ALIGN=\"right\"><span><font size=6>PMB 3.4 dans ses (tr&egrave;s) grandes lignes !</font><span>\r\n\r\n<h3><span>Acc&eacute;l&eacute;rez vos relances&nbsp;: historique, acc&egrave;s direct par lecteur, SMS !<span></h3>\r\n<UL>\r\n	<LI>les relances en cours sont accessibles via la fiche de l\'emprunteur,</li>\r\n	<LI>l\'historique des relances est d&eacute;sormais archiv&eacute;,</li>\r\n	<LI>les recouvrements sont accessibles par emprunteur en d&eacute;tail,</li>\r\n	<LI>possibilit&eacute; de changement automatique du statut de l\'emprunteur lors du passage en recouvrement, notamment pour lui interdire le pr&ecirc;t,</li>\r\n	<LI>param&eacute;trage des textes des lettres de relances par localisation,</li>\r\n	<LI>possibilit&eacute; d\'envoi des relances par SMS (connecteur selon fournisseur de service),</li>\r\n	<LI>enfin, travail sur les performances&nbsp;: les calculs d\'amendes de l\'ensemble des retardataires sont acc&eacute;l&eacute;r&eacute;s et mis en cache pour un r&eacute;affichage imm&eacute;diat.</li>\r\n</UL>\r\n\r\n<h3><span>Un auteur a beaucoup &eacute;crit sur un sujet&nbsp;: invitez votre lecteur &agrave; visiter les th&egrave;mes en rapport !<span></h3>\r\n<UL>\r\n	<LI>liens entre autorit&eacute;s&nbsp;: &agrave; l\'infini et entre toutes les autorit&eacute;s.</li>\r\n</UL>\r\n\r\n<h3><span>Transformez n\'importe quel poste en automate de pr&ecirc;t !<span></h3>\r\n<UL>\r\n	<LI>enregistrement autonome des pr&ecirc;ts par l\'emprunteur dans son compte OPAC&nbsp;: pour une meilleure tra&ccedil;abilit&eacute; des pr&ecirc;ts quand vous n\'&ecirc;tes pas l&agrave;.</li>\r\n</UL>\r\n\r\n<h3><span>Votre public &agrave; port&eacute;e de mobile&nbsp;: SmartPhone et SMS pour une information r&eacute;active !<span></h3>\r\n<UL>\r\n	<LI>votre fonds accessible &agrave; partir de n\'importe quel SmartPhone,</li>\r\n	<LI>r&eacute;servation d\'ouvrage et confirmation de disponibilit&eacute; par SMS,</li>\r\n	<LI>enregistrement autonome des pr&ecirc;ts par l\'emprunteur dans son compte,</li>\r\n	<LI>possibilit&eacute; de s&eacute;lectionner la biblioth&egrave;que la plus proche gr&acirc;ce &agrave; la fonction GPS !</li>\r\n</UL>\r\n\r\n<h3><span>RFID&nbsp;: toujours plus rapide et une meilleure prise en compte de la gestion de l\'antivol !<span></h3>\r\n<UL>\r\n	<LI>refonte de l\'ergonomie de gestion des pr&ecirc;ts multitags pour un enregistrement acc&eacute;l&eacute;r&eacute;,</li>\r\n	<LI>meilleure prise en compte de la gestion de l\'antivol pour &eacute;viter les fausses alertes aux portiques.</li>\r\n</UL>\r\n\r\n<h3><span>Imprimez vos bibliographies dans n\'importe quel format !<span></h3>\r\n<UL>\r\n	<LI>g&eacute;n&eacute;ralisation du syst&egrave;me de mod&egrave;les du bulletin de veille aux impressions, aux exports, aux &eacute;ditions personnalis&eacute;es.</li>\r\n</UL>\r\n\r\n<h3><span>Plus de webservices ! Une API enrichie pour une int&eacute;gration plus large aux CMS&nbsp;: Drupal en t&ecirc;te !<span></h3>\r\n<UL>\r\n	<LI>extensions des webservices,</li>\r\n	<LI>am&eacute;lioration des performances,</li>\r\n	<LI>acc&egrave;s aux th&eacute;saurus,</li>\r\n	<LI>application directe dans le CMS Drupal.</li>\r\n</UL>\r\n\r\n<h3><span>G&eacute;rez n\'importe quel carnet d\'adresses avec PMB !<span></h3>\r\n<UL>\r\n	<LI>nouveau module de gestion de fiches,</li>\r\n	<LI>carnet d\'adresses, partenaires, toute l\'information non bibliographique maintenant dans PMB.</li>\r\n</UL>\r\n\r\n<h3><span>Les petites choses qui font la diff&eacute;rence&nbsp;: lien entre navigation et plans de classement, champs personnalis&eacute;s d\'exemplaires&nbsp;:<span></h3>\r\n<UL>\r\n	<LI>association d\'un plan de classement &agrave; une section, pour que la navigation par cote reste coh&eacute;rente m&ecirc;me en environnement h&eacute;t&eacute;rog&egrave;ne,</li>\r\n	<LI>recherche multi-crit&egrave;res sur les champs personnalis&eacute;s d\'exemplaires.</li>\r\n</UL>\r\n\r\n<BR>\r\n\r\n<DIV TYPE=FOOTER>\r\n	<P ALIGN=CENTER STYLE=\"margin-top: 0.58cm; margin-bottom: 0cm; font-style: normal; font-weight: normal\">\r\n	<FONT COLOR=\"#666666\">PMB Services  &ndash;  ZA de Mont sur Loir  -  BP 10 023  &ndash;  72500\r\n	CHATEAU DU LOIR  -  FRANCE</FONT>\r\n	<P ALIGN=CENTER STYLE=\"margin-bottom: 0cm\"><FONT COLOR=\"#666666\">&#59013;</FONT><FONT COLOR=\"#666666\">&nbsp;<FONT SIZE=2 STYLE=\"font-size: 9pt\"><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">w</SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">ww.sigb.net\r\n	   </SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><FONT FACE=\"OpenSymbol\"><FONT SIZE=2><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">&#58375;</SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">&nbsp;+33(</SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">0)2\r\n	43 440 660    </SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><FONT FACE=\"OpenSymbol\"><FONT SIZE=2><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">&#57361;</SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">&nbsp;+33(0)2\r\n	43 444 132    </SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><FONT FACE=\"OpenSymbol\"><FONT SIZE=2><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">&#58377;</SPAN></SPAN></FONT><FONT COLOR=\"#666666\"><SPAN STYLE=\"font-style: normal\"><SPAN STYLE=\"font-weight: normal\">&nbsp;pmb@sigb.net</SPAN></SPAN>\r\n	<P ALIGN=CENTER STYLE=\"margin-bottom: 0cm; font-style: normal; font-weight: normal\">\r\n	<FONT COLOR=\"#666666\"><FONT FACE=\"Verdana, sans-serif\"><FONT SIZE=1>SARL\r\n	au capital de 3000&euro; / RCS Le Mans : 478 151 772</FONT>\r\n</DIV>", "PMB Version 3.4", 1, 0);
