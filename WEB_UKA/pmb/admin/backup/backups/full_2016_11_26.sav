#opac_sessions
drop table if exists opac_sessions;
CREATE TABLE `opac_sessions` (   `empr_id` int(10) unsigned NOT NULL DEFAULT '0',   `session` blob,   `date_rec` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,   PRIMARY KEY (`empr_id`) ) ENGINE=MyISAM DEFAULT CHARSET=latin1;
